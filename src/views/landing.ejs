<script>
document.addEventListener('DOMContentLoaded',function(){
    // Theme toggle
    var tb=document.getElementById('themeToggle'),ti=document.getElementById('themeIcon');
    function si(){ti.className=document.documentElement.getAttribute('data-theme')==='dark'?'fas fa-sun':'fas fa-moon'}
    si();
    tb.addEventListener('click',function(e){e.preventDefault();var d=document.documentElement.getAttribute('data-theme')==='dark',n=d?'light':'dark';document.documentElement.setAttribute('data-theme',n);localStorage.setItem('booknotes-theme',n);si()});

    // Smooth scroll
    var lm=document.getElementById('learnMoreBtn');
    if(lm)lm.addEventListener('click',function(e){e.preventDefault();var t=document.getElementById('features');if(t)t.scrollIntoView({behavior:'smooth',block:'start'})});

    // Mobile menu
    var mt=document.getElementById('mobileToggle'),mm=document.getElementById('mobileMenu'),mi=document.getElementById('mobileIcon');
    if(mt&&mm){mt.addEventListener('click',function(){var o=mm.classList.contains('open');mm.classList.toggle('open');mi.className=o?'fas fa-bars':'fas fa-times'});
    mm.querySelectorAll('a').forEach(function(a){a.addEventListener('click',function(){mm.classList.remove('open');mi.className='fas fa-bars'})})}

    // Intersection Observer for scroll animations
    var io=new IntersectionObserver(function(entries){entries.forEach(function(e){if(e.isIntersecting)e.target.classList.add('vis')})},{threshold:.08,rootMargin:'0px 0px -40px 0px'});
    document.querySelectorAll('.ao').forEach(function(el){io.observe(el)});
});
</script>
</body>
</html>
